{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" },
    { "source": "/api/coupons/claim", "destination": "/api/coupons?action=claim" }

    
  
  ]
}
